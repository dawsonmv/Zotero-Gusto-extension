{
  "manifest_version": 2,
  "name": "Gusto - Robust Links for Zotero",
  "version": "3.0.0",
  "description": "Archive web references using Memento Web framework",
  "author": "Los Alamos National Laboratory",
  "homepage_url": "https://github.com/dawsonmv/Zotero-Gusto-extension",
  
  "applications": {
    "zotero": {
      "id": "zotero-robustlinks@mementoweb.org",
      "update_url": "https://raw.githubusercontent.com/dawsonmv/Zotero-Gusto-extension/main/update.json",
      "strict_min_version": "7.0",
      "strict_max_version": "7.0.*"
    }
  },
  
  "icons": {
    "48": "chrome/content/robustlinks/icon48.png",
    "96": "chrome/content/robustlinks/icon96.png"
  }
}